{
  "language": "zh-hans",


  "plugins": [
    "analytics",
    "github",
    "tbfed-pagefooter",
    "anchor-navigation-ex@0.1.8",
    "-sharing"

  ],

  "pluginsConfig": {
    "github": {
      "url": "https://github.com/exchristian/exchristian.github.io"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy exchristian 2018",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "fontsettings": {
      "size": 1
    },
    "analytics": {
      "google": "UA-126653067-1",
      "baidu":"d8516583d24f8363544591b2707f470b"
    },
    "pdf": {
      "pageNumbers": true,
      "fontSize": 18,
      "paperSize": "a5"

    }

  }
}